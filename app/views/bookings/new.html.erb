<h1>Bookings#new</h1>
<p>Find me in app/views/bookings/new.html.erb</p>


<%= simple_form_for @booking do |f| %>
  <%= f.input :start_date %>
  <%= f.input :end_date %>
  <%= f.input :payment_amount %>
  <%= f.input :flat_id %>
  <%= f.input :user_id %>
  <%= f.input :flat_review_id %>
  <%= f.button :submit, class: "btn btn-primary"%>
<% end %>
